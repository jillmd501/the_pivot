<h1>Create a City</h1>

<%= form_for City.new, url: admin_cities_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :short_d %>
  <%= f.text_field :short_d %>
  <%= f.label :long_d %>
  <%= f.text_field :long_d %>
  <%= f.label :image_path %>
  <%= f.text_field :image_path %>
  <%= f.submit "Continue Creating a Trip" %>
<% end %>
