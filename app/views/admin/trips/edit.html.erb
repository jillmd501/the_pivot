<h1>Update a Trip</h1>
<%= form_for @trip, url: admin_city_trip_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label :image_path %>
  <%= f.text_field :image_path %>
  <%= f.label :info %>
  <%= f.text_field :info %>
  <%= f.submit "Update Trip" %>
<% end  %>
