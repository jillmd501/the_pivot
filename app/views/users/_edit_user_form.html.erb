<%= form_for(@user) do |f| %>
  <%= f.label(:email) %>
  <%= f.text_field(:email) %>

  <%= f.label(:first_name) %>
  <%= f.text_field(:first_name) %>

  <%= f.label(:last_name) %>
  <%= f.text_field(:last_name) %>

  <%= f.label(:password) %>
  <%= f.text_field(:password) %>
  
  <%= f.submit("Update Profile") %>
<% end %>